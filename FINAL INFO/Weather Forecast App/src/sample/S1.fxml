<AnchorPane prefHeight="500" prefWidth="500" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.S1Cont">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" layoutY="20.0" prefHeight="91.0" prefWidth="400.0" text="Weather Forecast" textAlignment="CENTER">
         <font>
            <Font name="Arial Bold" size="45.0" />
         </font>
      </Label>
      <Button fx:id="Update" alignment="CENTER" layoutX="279.0" layoutY="389.0" mnemonicParsing="false" onAction="#update" prefHeight="40.0" prefWidth="91.0" text="Update" textAlignment="CENTER">
         <font>
            <Font name="Heiti TC Light" size="18.0" />
         </font>
      </Button>
      <Button layoutX="159.0" layoutY="389.0" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="91.0" text="Back">
         <font>
            <Font name="Heiti TC Light" size="18.0" />
         </font></Button>
      <Label fx:id="TownName" alignment="CENTER" contentDisplay="CENTER" layoutX="191.0" layoutY="133.0" prefHeight="46.0" prefWidth="135.0">
         <font>
            <Font size="21.0" />
         </font></Label>
      <Circle fill="DODGERBLUE" layoutX="261.0" layoutY="230.0" radius="51.0" stroke="BLACK" strokeType="INSIDE" />
      <Label fx:id="RealTemp" alignment="CENTER" contentDisplay="CENTER" layoutX="219.0" layoutY="184.0">
         <font>
            <Font size="57.0" />
         </font>
      </Label>
      <Label fx:id="FeelsTemps" alignment="CENTER" contentDisplay="CENTER" layoutX="218.0" layoutY="305.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="Condition" alignment="CENTER" contentDisplay="CENTER" layoutX="241.0" layoutY="253.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
